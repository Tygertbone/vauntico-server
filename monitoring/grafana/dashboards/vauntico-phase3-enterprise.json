{
  "dashboard": {
    "id": null,
    "title": "Vauntico Phase 3 - Enterprise Compliance & Integrations",
    "tags": ["vauntico", "phase3", "enterprise", "compliance"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Enterprise Compliance Overview",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "vauntico_compliance_score",
            "legendFormat": "Compliance Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 70
                },
                {
                  "color": "green",
                  "value": 90
                }
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        }
      },
      {
        "id": 2,
        "title": "POPIA Compliance Events",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "rate(vauntico_compliance_events_total{framework=\"popia\"}[5m])",
            "legendFormat": "POPIA Events Rate"
          }
        ],
        "yAxes": [
          {
            "label": "Events per Second"
          }
        ]
      },
      {
        "id": 3,
        "title": "Data Privacy Requests",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "vauntico_privacy_requests_total",
            "legendFormat": "Total Privacy Requests"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "title": "GDPR Alignment",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 8
        },
        "targets": [
          {
            "expr": "vauntico_gdpr_compliance_score",
            "legendFormat": "GDPR Compliance"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 80
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Enterprise MRR Growth",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "vauntico_enterprise_mrr",
            "legendFormat": "Enterprise MRR ($)"
          }
        ],
        "yAxes": [
          {
            "label": "USD"
          }
        ]
      },
      {
        "id": 6,
        "title": "Integration Health Status",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 16
        },
        "targets": [
          {
            "expr": "vauntico_integration_health{integration=\"slack\"}",
            "legendFormat": "Slack"
          },
          {
            "expr": "vauntico_integration_health{integration=\"notion\"}",
            "legendFormat": "Notion"
          },
          {
            "expr": "vauntico_integration_health{integration=\"webhooks\"}",
            "legendFormat": "Webhooks"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 0.5
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Webhook Delivery Rate",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 16
        },
        "targets": [
          {
            "expr": "rate(vauntico_webhook_deliveries_success_total[5m])",
            "legendFormat": "Successful Delivery Rate"
          },
          {
            "expr": "rate(vauntico_webhook_deliveries_failed_total[5m])",
            "legendFormat": "Failed Delivery Rate"
          }
        ],
        "yAxes": [
          {
            "label": "Deliveries per Second"
          }
        ]
      },
      {
        "id": 8,
        "title": "Data Retention Compliance",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "targets": [
          {
            "expr": "vauntico_data_retention_compliance",
            "legendFormat": "{{data_category}}",
            "format": "table"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Enterprise Users",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 24
        },
        "targets": [
          {
            "expr": "vauntico_enterprise_users_total",
            "legendFormat": "Enterprise Users"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 10,
        "title": "Compliance Violations",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 24
        },
        "targets": [
          {
            "expr": "vauntico_compliance_violations_total",
            "legendFormat": "Violations"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 5
                },
                {
                  "color": "red",
                  "value": 10
                }
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Trust Score Distribution (Enterprise)",
        "type": "histogram",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        },
        "targets": [
          {
            "expr": "rate(vauntico_trust_score_enterprise_bucket[5m])",
            "legendFormat": "{{le}}"
          }
        ]
      },
      {
        "id": 12,
        "title": "Audit Log Events",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        },
        "targets": [
          {
            "expr": "rate(vauntico_audit_events_total{type=\"enterprise\"}[5m])",
            "legendFormat": "Enterprise Audit Events"
          },
          {
            "expr": "rate(vauntico_audit_events_total{type=\"compliance\"}[5m])",
            "legendFormat": "Compliance Audit Events"
          }
        ],
        "yAxes": [
          {
            "label": "Events per Second"
          }
        ]
      },
      {
        "id": 13,
        "title": "SLA Compliance (99.9% Target)",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 32
        },
        "targets": [
          {
            "expr": "vauntico_sla_uptime_percentage",
            "legendFormat": "Uptime SLA"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 99.0
                },
                {
                  "color": "yellow",
                  "value": 99.5
                },
                {
                  "color": "green",
                  "value": 99.9
                }
              ]
            }
          }
        }
      },
      {
        "id": 14,
        "title": "Cross-Border Data Transfers",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 32
        },
        "targets": [
          {
            "expr": "vauntico_cross_border_transfers_total",
            "legendFormat": "Cross-Border Transfers"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "links": [
      {
        "title": "Enterprise Documentation",
        "url": "/docs/enterprise-compliance"
      },
      {
        "title": "Compliance Reports",
        "url": "/api/v1/enterprise/compliance/reports"
      }
    ]
  }
}
