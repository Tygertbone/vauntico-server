    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {},
    "refresh": "30s"
  },
  "templating": {
    "list": {
      "type": "conduit",
      "datasource": "prometheus",
      "step": 20
    },
      "options": {
        "series": [
          {
            "alias": "Sponsorships",
            "datasource": "prometheus",
            "measurement": "vauntico_sponsorships_active_total",
            "refId": "A"
          },
          {
            "alias": "Marketplace Items",
            "datasource": "prometheus",
            "measurement": "vauntico_marketplace_items_active_total",
            "refId": "B"
          },
          {
            "alias": "Love Loops",
            "datasource": "prometheus",
            "measurement": "vauntico_love_loops_total",
            "refId": "C"
          },
          {
            "alias": "Echo Stories",
            "datasource": "prometheus",
            "measurement": "vauntico_echo_chamber_stories_total",
            "refId": "D"
          },
          {
            "alias": "Engagement Activities",
            "datasource": "prometheus",
            "measurement": "vauntico_community_engagement_activities_total",
            "refId": "E"
          }
        ]
      },
      "annotations": {
        "list": [
          {
            "datasource": "prometheus",
            "enable": true,
            "expr": "up(vauntico_sponsorships_active_total)",
            "text": "Active Sponsorships",
            "title": "Number of active sponsorships",
            "colorMode": "value",
            "icon": "pulse"
          },
          {
            "datasource": "prometheus",
            "enable": true,
            "expr": "up(vauntico_marketplace_items_active_total)",
            "text": "Active Marketplace Items",
            "title": "Number of active marketplace items",
            "colorMode": "value",
            "icon": "package"
          },
          {
            "datasource": "prometheus",
            "enable": true,
            "expr": "up(vauntico_love_loops_total)",
            "text": "Total Love Loops",
            "title": "Total number of love loops created",
            "colorMode": "value",
            "icon": "heart"
          },
          {
            "datasource": "prometheus",
            "enable": true,
            "expr": "up(vauntico_echo_chamber_stories_total)",
            "text": "Published Echo Stories",
            "title": "Number of published echo chamber stories",
            "colorMode": "value",
            "icon": "edit"
          },
          {
            "datasource": "prometheus",
            "enable": true,
            "expr": "up(vauntico_community_engagement_activities_total)",
            "text": "Engagement Activities",
            "title": "Total community engagement activities",
            "colorMode": "value",
